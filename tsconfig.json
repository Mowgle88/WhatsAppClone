// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json", /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "typeRoots": [
      "node_modules/@types",
      "src/shared/types"
    ],
    /* Completeness */
    "skipLibCheck": true, /* Skip type checking all .d.ts files. */
    "baseUrl": "./src",
    "paths": {
      "#assets/*": [
        "./assets/*"
      ],
      "#navigation": [
        "./navigation"
      ],
      "#navigation/*": [
        "./navigation/*"
      ],
      "#screens": [
        "./screens"
      ],
      "#store": [
        "./store"
      ],
      "#store/*": [
        "./store/*"
      ],
      "#components": [
        "./shared/components"
      ],
      "#constants": [
        "./shared/constants"
      ],
      "#colors": [
        "./shared/constants/colors"
      ],
      "#styles": [
        "./shared/constants/commonStyles"
      ],
      "#hooks": [
        "./shared/hooks"
      ],
      "#permissions": [
        "./shared/permissions"
      ],
      "#types": [
        "./shared/types"
      ],
      "#utils": [
        "./shared/utils"
      ],
      "#ui": [
        "./shared/ui"
      ]
    }
  }
}
